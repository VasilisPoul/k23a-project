{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "project",  
            "type": "cppdbg",
            "request": "launch",
            "preLaunchTask": "Build Make",
            // "program": "${workspaceFolder}/project", //path to your programs exe and exe name
            "program": "${workspaceFolder}/project",
            "args": [
                "-dir",
                "Datasets/camera_specs/2013_camera_specs",
                "-csv",
                "Datasets/sigmod_medium_labelled_dataset.csv"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
